#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/marine/matria-marine-website/node_modules/.pnpm/baseline-browser-mapping@2.8.13/node_modules/baseline-browser-mapping/dist/node_modules:/mnt/c/marine/matria-marine-website/node_modules/.pnpm/baseline-browser-mapping@2.8.13/node_modules/baseline-browser-mapping/node_modules:/mnt/c/marine/matria-marine-website/node_modules/.pnpm/baseline-browser-mapping@2.8.13/node_modules:/mnt/c/marine/matria-marine-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/marine/matria-marine-website/node_modules/.pnpm/baseline-browser-mapping@2.8.13/node_modules/baseline-browser-mapping/dist/node_modules:/mnt/c/marine/matria-marine-website/node_modules/.pnpm/baseline-browser-mapping@2.8.13/node_modules/baseline-browser-mapping/node_modules:/mnt/c/marine/matria-marine-website/node_modules/.pnpm/baseline-browser-mapping@2.8.13/node_modules:/mnt/c/marine/matria-marine-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../baseline-browser-mapping@2.8.13/node_modules/baseline-browser-mapping/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../baseline-browser-mapping@2.8.13/node_modules/baseline-browser-mapping/dist/cli.js" "$@"
fi
